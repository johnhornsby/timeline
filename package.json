{
  "name": "timeline",
  "version": "1.0.0",
  "description": "Agnostic logic for managing a timeline, adding keyframes and tweening between them.",
  "main": "dist/timeline.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack",
    "build": "npm run build:lib && npm run build:umd",
    "test": "jest"
  },
  "author": "john@interactivelabs.co.uk",
  "license": "ISC",
  "test": "jest --verbose",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "unmockedModulePathPatterns": ["<rootDir>/src/timeline"]
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "jest-cli": "^0.8.2",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "motion-tween": "git+https://github.com/johnhornsby/motion-tween.git#9c96ca6",
    "wolfy87-eventemitter": "^4.2.11"
  }
}
